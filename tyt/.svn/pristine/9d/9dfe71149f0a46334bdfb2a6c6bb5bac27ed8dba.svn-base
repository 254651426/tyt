.WdateDiv {
	width: 180px;
	padding: 1px;
	margin-left: 2px;
	margin-top: 2px;
	filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#cccccc');
	-ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#cccccc')";
	border: #779cb0 1px solid;
	background-color: #ffffff;
}

.WdateDiv2 {
	width: 360px;
}

.WdateDiv * {
	font-size: 9pt;
}

.WdateDiv .NavImg a {
	cursor: pointer;
	display: block;
	width: 16px;
	height: 16px;
	margin-top: 1px;
}

.WdateDiv .NavImgll a {
	float: left;
	background: url(img.gif) no-repeat;
}

.WdateDiv .NavImgl a {
	float: left;
	background: url(img.gif) no-repeat -16px 0px;
}

.WdateDiv .NavImgr a {
	float: right;
	background: url(img.gif) no-repeat -32px 0px;
}
.WdateDiv .NavImgrr a{
	float: right;
	background: url(img.gif) no-repeat -48px 0px;
}

.WdateDiv #dpTitle {
	height: 24px;
	padding: 1px;
	background: url(bg.gif);
}

.WdateDiv .yminput {
	margin-top: 1px;
	text-align: center;
	border: 0px;
	height: 20px;
	width: 50px;
	color: #034c50;
	background-color: transparent;
	cursor: pointer;
}

.WdateDiv .yminputfocus {
	margin-top: 1px;
	text-align: center;
	border: #c3d6e8 1px solid;
	font-weight: bold;
	color: #034c50;	
	height: 20px;
	width: 50px;
}

.WdateDiv .menuSel {
	display: none;
	position: absolute;
	z-index: 1;
	border: #a3c6c8 1px solid;
	background-color: #ffffff;
}

.WdateDiv .menu {
	line-height: 14px;
	color: #11777c;
	cursor: pointer;
	background-color: #fffff;
}

.WdateDiv .menuOn {
	line-height: 14px;
	cursor: pointer;
	background-color: #beebee;
}

.WdateDiv .invalidMenu {
	color: #aaaaaa;
}

.WdateDiv .YMenu {
	margin-top: 21px;
}

.WdateDiv .MMenu {
	margin-top: 21px;
	*width: 62px;
}

.WdateDiv .hhMenu {
	margin-top: -90px; 
	margin-left: 26px;
}

.WdateDiv .mmMenu {
	margin-top: -46px; 
	margin-left: 26px;
}

.WdateDiv .ssMenu {
	margin-top: -24px; 
	margin-left: 26px;
}

.WdateDiv .Wweek {
	text-align: center;
	background: #daf3f5;
	border-right: #bdebee 1px solid;
}

.WdateDiv .MTitle {
	color: #848484;
	border-bottom: #bdebee 1px solid;
}

.WdateDiv .WdayTable2 {
	border: #bee9f0 1px solid;
}

.WdateDiv .WdayTable2 table {
	border: 0px;
}

.WdateDiv .WdayTable {
	line-height: 20px;	
	color: #13777e;
	border-spacing: 1px;
	border-spacing: expression(this.cellSpacing=1);
	background-color: #f6fafd;
}

.WdateDiv .WdayTable td {
	text-align: center;
}

.WdateDiv .Wday {
	cursor: pointer;
	border: 1px solid #f6fafd;
}

.WdateDiv .WdayOn {
	cursor: pointer;
	border: 1px solid #79b7e7;
	background-color: #d0e5f5;
}

.WdateDiv .Wwday {
	color: #ab1e1e;
	cursor: pointer;
	border: 1px solid #f6fafd;
}

.WdateDiv .WwdayOn {
	cursor: pointer;
	border: 1px solid #79b7e7;
	background-color: #d0e5f5;
}
.WdateDiv .Wtoday {
	color: #000000;
	cursor: pointer;
	border: 1px solid #fad42e;
	background-color: #fbec88;
}
.WdateDiv .Wselday {
	color: #000000;
	font-weight: bold;
	border: 1px solid #79b7e7;
	background-color: #d0e5f5;
}
.WdateDiv .WspecialDay {
	background-color: #66f4df;
}

.WdateDiv .WotherDay { 
	cursor: pointer;
	color: #0099CC;	
}

.WdateDiv .WotherDayOn { 
	cursor: pointer;
	background-color: #c0ebef;	
}

.WdateDiv .WinvalidDay {
	color: #aaa;
}

.WdateDiv #dpTime {
	float: left;
	margin-top: 3px;
	margin-right: 30px;
}

.WdateDiv #dpTime #dpTimeStr {
	margin-left: 1px;
	color: #497f7f;
}

.WdateDiv #dpTime input {
	height: 20px;
	width: 18px;
	text-align: center;
	color: #333;
	border: #61cad0 1px solid;	
}

.WdateDiv #dpTime .tB {
	border-right:0px;
}

.WdateDiv #dpTime .tE {
	border-left: 0;
	border-right: 0;
}

.WdateDiv #dpTime .tm {
	width:7px;
	border-left:0;
	border-right:0;
}

.WdateDiv #dpTime #dpTimeUp {
	height: 10px;
	width: 13px;
	border: 0px;
	background: url(img.gif) no-repeat -32px -16px;
}

.WdateDiv #dpTime #dpTimeDown {
	height: 10px;
	width: 13px;
	border: 0px;
	background: url(img.gif) no-repeat -48px -16px;
}

.WdateDiv #dpQS {
 	float: left;
	margin-right: 3px;
	margin-top: 3px;
	background: url(img.gif) no-repeat 0px -16px;
	width: 20px;
	height: 20px;
	cursor: pointer;
}

.WdateDiv #dpControl {
	text-align: center;
	margin-top: 3px;
}

.WdateDiv .dpButton { 
	height: 20px;
	width: 45px;
	margin: 2px;
	border: #38b1b9 1px solid;
	background-color: #cfebee;
	color: #08575b;
}