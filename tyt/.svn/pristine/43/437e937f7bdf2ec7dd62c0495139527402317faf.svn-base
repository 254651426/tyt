package com.bquan.entity.mysql;

import java.io.Serializable;
import java.util.Date;

/**
 * 佣金率 Entity
 * @author liuxiaokang
 * @createTime 2016-08-22
 */
public class CommissionRate extends BaseStrEntity{

	private String username;//
	private Integer writeRate;//
	private Integer showRate;//
	private Integer limitSendTime;// 推广渠道最高赠送时间
	
	private String productAmount;// 非数据库字段，交易金额
	private String sendAmount;// 非数据库字段，佣金金额
	private String todayAmount;// 非数据库字段，今日佣金金额
	private String realAmount;
 
	public String getUsername() {  
        return username;  
    }  
    public void setUsername(String username) {  
        this.username = username;  
    }
	public Integer getWriteRate() {  
        return writeRate;  
    }  
    public void setWriteRate(Integer writeRate) {  
        this.writeRate = writeRate;  
    }
	public Integer getShowRate() {  
        return showRate;  
    }  
    public void setShowRate(Integer showRate) {  
        this.showRate = showRate;  
    }
	public String getProductAmount() {
		return productAmount;
	}
	public void setProductAmount(String productAmount) {
		this.productAmount = productAmount;
	}
	public String getSendAmount() {
		return sendAmount;
	}
	public void setSendAmount(String sendAmount) {
		this.sendAmount = sendAmount;
	}
	public String getTodayAmount() {
		return todayAmount;
	}
	public void setTodayAmount(String todayAmount) {
		this.todayAmount = todayAmount;
	}
	public Integer getLimitSendTime() {
		return limitSendTime;
	}
	public void setLimitSendTime(Integer limitSendTime) {
		this.limitSendTime = limitSendTime;
	}
	public String getRealAmount() {
		return realAmount;
	}
	public void setRealAmount(String realAmount) {
		this.realAmount = realAmount;
	}
	
}
