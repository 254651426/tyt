@charset "utf-8";
/* CSS Document */
input[type=button], input[type=submit], input[type=file], button { cursor: pointer; -webkit-appearance: none; } 
.bg{ background:url(../images/bg.jpg) no-repeat top;background-size: contain;min-height: 560px;}
.logo{text-align:center;}
.img-circle{
    background: #fff;
    z-index: 1000;
    position: inherit;
    margin-top: 17%;
    border: 1px solid rgba(52, 73, 94, .44);
    padding: 4px;
	width: 25%;
    margin-bottom: 20px;
}

.login-username-w{margin-top:30%;}
.login-input{position:relative;}
.login-name{border:2px solid #e2e2e2;;padding:12px 0px;border-radius:50px;width:100%;margin:10px 0px;display: block;text-indent: 30px;font-size:18px;}
.login-button{border-radius:50px;background-color:#457cfa;width:100%;border:none;;padding:12px 0px;margin:10px 0px;color:#fff;font-size:20px;}
.login-username-icon{
    position: absolute;
    display: block;
    line-height: 1;
    z-index: 2;
	margin-left: 10px;
    margin-top: 5%;
}
.login-username{z-index: 1;position:absolute;}
.login-forgetpwd{color:#918f8f;width:100%;text-align:right; display:block;}
.forgetpwd-w{margin-top:60%;}
.forgetpwd-bg{background-color:#fff; }
.forgetpwd-bg-img{background:url(../images/forger_img.jpg) no-repeat;background-size: contain;min-height:560px;}
.forgetpwd-in{border:1px solid #ccc;border-radius:5px;background-color:#fff;width: 100%;padding:12px 0px;margin:5px 0px;text-indent:10px;font-size:18px;}
.forgetpwd-getcode-button{border:1px solid #f6aa2c;border-radius:5px;background-color:#f6aa2c;width: 100%;padding:12px 0px;margin:5px 0px;font-size:18px;color:#fff;}
.forgetpwd-getcode-button:focus{border-radius:5px;background-color:#ccc;}
.forgetpwd-button{border-radius:5px;background-color:#457cfa;width:100%;border:none;padding:10px 0px;margin:10px 0px;color:#fff;font-size:18px;}
.forgetpwd-getcode{width:50%;float:right;}
.forgetpwd-getcode-input{width:48%;float:left;}

/*information*/
.information-img {
    background: url(../images/information_top.jpg) no-repeat;
    background-size: contain;padding-right: 0px;
    padding-left: 0px;
    min-height: 100px;
    width: 100%;
	 background-size: cover;
}
.information-bg{background-color:#f8f6f9; }
.information-w{
    background-color: #fff;    border-top: solid 1px #ebecec;
    border-bottom: solid 1px #ebecec;
	margin: 10px 0px;
	}
.information-title {
    padding: 10px;
    border-bottom: 1px solid #ebecec;
    overflow: auto;
}
.information-title-topb {
    padding: 10px;
    border-bottom: 1px solid #ebecec;
	border-top: 1px solid #ebecec;
    overflow: auto;
}
.information-left {
    float: left;
	padding: 3px 0px;
}
.information-inp {
    float: left;
    color: #999999;
    text-align: right;
    padding-right: 10px;
}
.information-inp input{border:none;padding:3px 0px;}
.information-getcode-button {
	background-color: #f6aa2c;
    border-radius: 5px;
    padding: 12px 0px;
    /* margin: 5px 0px; */
    font-size: 18px;
    color: #fff;
}
.information-submit{margin:10px 20px;}
.info-LUploader {
    text-align: center;
    border-radius: 4px;
    border: 1px dashed #dedde2;
    background: #ffffff;
    position: relative;
	float: left;
}
.info-user-img{padding: 10px 0px;padding-left:20px;}
.info-LUploader .LUploader-container,LUploader-list {
    height: 100%;
}
.info-LUploader .icon {
    font-size: 20px;
    margin: 1.5rem auto 5px auto;
height: 2em;
    display: block;
}
.info-LUploader input[data-LUploader] {
    display: none;
}
.info-img-circle {
    background: #fff;
    position: inherit;
    border: 1px solid rgba(52, 73, 94, .44);
    padding: 3px;
    width: 75%;
}
.info-img-circle {
    border-radius: 50%;
}
.info-user-img-l{ float:left;width:30%;}
.info-user-img-info{ float:left;color:#fff;margin-top: 1rem;
    font-size: 20px;}
.info-user-img-info span{color:#fff;}
.info-icon-camera{ background:url(../images/camera.png) no-repeat center center;background-size: 32px 32px;}
.info-message-error{margin:3px; background-color:#ffffe4;padding: 5px 6px;}
.info-message-error-img{ width:26px;}
.info-message-error span{padding-left:10px;color:#565656;}
/*saoma*/
.saoma_bg{ background:url(../images/saoma_img2.jpg) no-repeat top center;background-size: cover;}
.saoma_ma{position: absolute;text-align:center;
    width: 100%;
    height: 100%;}
.saoma_ma img{width: 50%; margin:0 auto;margin-top: 50%;border: 5px solid #fff;box-shadow:0px 0px 10px 3px #1F212D;}
.saoma-fontstyle{color:#fff;text-align:center;font-size:20px;line-height:5rem;}

.success-img{top: 50%;position: absolute;width: 100%;}
.success-img img{width:100%;margin-top: 50%;}
.success-img-button{
	border-radius: 2px;
    color: #fff;
    background-color: #5eb95e;
    border-color: #5eb95e;
    padding-left: 0;
    padding-right: 0;
}
.am-btn-block{
    width: 100%;
    display: block;
}
.tip_content{text-align: center;font-size:18px;}
.changeusername{width: 90%;
    margin: 10px auto;}
.success-am-btn {
    display: inline-block;
    margin-bottom: 0;
    padding: .7em 1em;
    font-size: 1.6rem;
    font-weight: 400;
    line-height: 1.2;
    text-align: center;
    white-space: nowrap;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 3px;
    cursor: pointer;
    outline: 0;
    -webkit-appearance: none;
    user-select: none;
    -webkit-transition: background-color 300ms ease-out, border-color 300ms ease-out;
    transition: background-color 300ms ease-out, border-color 300ms ease-out;
}
.error-button {
    border-radius: 2px;
    color: #fff;
    background-color: #bbbbbb;
    border-color: #bbbbbb;
}
/*弹框*/
#dialog {
            display: none;
        }
#dialog2 {
            display: none;
        }

        .weui-mask {
            position: fixed;
            z-index: 1000;
            top: 0;
            right: 0;
            left: 0;
            bottom: 0;
            background: rgba(0,0,0,.6);
        }

        .weui-dialog {
            position: fixed;
            z-index: 5000;
            width: 80%;
            top: 50%;
            left: 50%;
            -webkit-transform: translate(-50%,-50%);
            transform: translate(-50%,-50%);
            background-color: #fff;
            text-align: center;
            border-radius: 3px;
            overflow: hidden;
        }

        .weui-dialog__bd {
            padding: 2.7em 2rem 1.7em;
            font-size: 1.5rem;
            line-height: 1.3;
            word-wrap: break-word;
            word-break: break-all;
            color: #353535;
			border-bottom: 1px solid #eaeaeb;

        }

        .weui-dialog__ft {
			border-right: 1px solid #eaeaeb;
    float: left;
    width: 49%;
            position: relative;
            line-height: 4.8rem;
            font-size: 1.2rem;
            display: -webkit-box;
            display: -webkit-flex;
            display: flex;
        }
		  .weui-dialog__rt {
    float: left;
    width: 50%;
            position: relative;
            line-height: 4.8rem;
            font-size: 1.2rem;
            display: -webkit-box;
            display: -webkit-flex;
            display: flex;
        }
.weui-dialog__ft2 {
			border-right: 1px solid #eaeaeb;
    float: left;
    width: 100%;
            position: relative;
            line-height: 4.8rem;
            font-size: 1.8rem;
            display: -webkit-box;
            display: -webkit-flex;
            display: flex;
        }

            .weui-dialog__ft2:after {
                content: "";
                position: absolute;
                left: 0;
                top: 0;
                right: 0;
                height: 1px;
                border-top: 1px solid #d5d5d6;
                color: #d5d5d6;
                -webkit-transform-origin: 0 0;
                transform-origin: 0 0;
                -webkit-transform: scaleY(0.5);
                transform: scaleY(0.5);
            }

        .weui-dialog__btn {
            display: block;
            -webkit-box-flex: 1;
            -webkit-flex: 1;
            flex: 1;
            color: #0BB20C;
            text-decoration: none;
            -webkit-tap-highlight-color: transparent;
            position: relative;
        }

            .weui-dialog__btn:active {
                background-color: #EEE;
            }
            
        .weui-dialog__btn2 {
            display: block;
            -webkit-box-flex: 1;
            -webkit-flex: 1;
            flex: 1;
            color: #0BB20C;
            text-decoration: none;
            -webkit-tap-highlight-color: transparent;
            position: relative;
        }

            .weui-dialog__btn2:active {
                background-color: #EEE;
            }
.tabs {
  width: 100%;
  float: none;
  list-style: none;
  position: relative;
  text-align: left;
}
.tabs li {
  float: left;
  display: block;
}
.tabs input[type="radio"] {
  position: absolute;
}
.tabs label {
	padding: 15px;
    text-align: center;
margin-left: 10px;
border:1px solid #A1C1D4;
height: 55px;
width: 100px;
  display: block;
  border-radius: 2px 2px 0 0;
  font-size: 16px;
  font-weight: normal;
  text-transform: uppercase;
  cursor: pointer;
  position: relative;
  top: 4px;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
}
.tabs .tab-content {
  background-color:#fff;
  z-index: 2;
  display: none;
  overflow: hidden;
  width: 100%;
  font-size: 17px;
  line-height: 25px;
  padding: 25px;
  position: absolute;
  top: 53px;
  left: 0;
  border:1px solid #A1C1D4;
}
.tabs [id^="tab"]:checked + label {
  top: 0;
  border:1px solid #A1C1D4;
  border-top:3px solid #A1C1D4;
   border-bottom: 3px solid #fff;
    z-index: 999;

}
.tabs [id^="tab"]:checked ~ [id^="tab-content"] {
  display: block;
}	
			
